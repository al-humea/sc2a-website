{"ast":null,"code":"import FooterBar from '@/components/FooterBar.vue';\nexport default {\n  name: \"Contact\",\n  components: {\n    FooterBar\n  },\n  data() {\n    return {\n      captcha: true\n    };\n  },\n  computed: {\n    objet: {\n      get() {\n        return this.$store.state.obj;\n      },\n      set(value) {\n        this.$store.commit(\"UPD_OBJ\", value);\n      }\n    },\n    statut: {\n      get() {\n        return this.$store.state.pro;\n      },\n      set(value) {\n        this.$store.commit(\"UPD_STATUS\", value);\n      }\n    },\n    soc: {\n      get() {\n        return this.$store.state.soc;\n      },\n      set(value) {\n        this.$store.commit(\"UPD_SOC\", value);\n      }\n    },\n    nom: {\n      get() {\n        return this.$store.state.nom;\n      },\n      set(value) {\n        this.$store.commit(\"UPD_NOM\", value);\n      }\n    },\n    pren: {\n      get() {\n        return this.$store.state.pren;\n      },\n      set(value) {\n        this.$store.commit(\"UPD_PRE\", value);\n      }\n    },\n    tel: {\n      get() {\n        return this.$store.state.tel;\n      },\n      set(value) {\n        this.$store.commit(\"UPD_TEL\", value);\n      }\n    },\n    email: {\n      get() {\n        return this.$store.state.email;\n      },\n      set(value) {\n        this.$store.commit(\"UPD_EMAIL\", value);\n      }\n    },\n    content: {\n      get() {\n        return this.$store.state.content;\n      },\n      set(value) {\n        this.$store.commit(\"UPD_CONT\", value);\n      }\n    }\n  }\n};","map":{"version":3,"names":["FooterBar","name","components","data","captcha","computed","objet","get","$store","state","obj","set","value","commit","statut","pro","soc","nom","pren","tel","email","content"],"sources":["/home/ascend/taff/web/sc2a-website/src/views/Contact.vue"],"sourcesContent":["<script>\nimport FooterBar from '@/components/FooterBar.vue';\nexport default {\n    name: \"Contact\",\n    components: { FooterBar },\n    data() {\n        return ({\n            captcha: true\n        });\n    },\n    computed: {\n        objet: {\n            get() { return this.$store.state.obj; },\n            set(value) { this.$store.commit(\"UPD_OBJ\", value); }\n        },\n        statut: {\n            get() { return this.$store.state.pro; },\n            set(value) { this.$store.commit(\"UPD_STATUS\", value); }\n        },\n        soc: {\n            get() { return this.$store.state.soc; },\n            set(value) { this.$store.commit(\"UPD_SOC\", value); }\n        },\n        nom: {\n            get() { return this.$store.state.nom; },\n            set(value) { this.$store.commit(\"UPD_NOM\", value); }\n        },\n        pren: {\n            get() { return this.$store.state.pren; },\n            set(value) { this.$store.commit(\"UPD_PRE\", value); }\n        },\n        tel: {\n            get() { return this.$store.state.tel; },\n            set(value) { this.$store.commit(\"UPD_TEL\", value); }\n        },\n        email: {\n            get() { return this.$store.state.email; },\n            set(value) { this.$store.commit(\"UPD_EMAIL\", value); }\n        },\n        content: {\n            get() { return this.$store.state.content; },\n            set(value) { this.$store.commit(\"UPD_CONT\", value); }\n        }\n    }\n}\n</script>\n\n<template>\n\t<div>\n\t\t<form v-if=\"captcha\" id=\"contact\" class=\"content\">\n\t\t\t<label for=\"dropdown\">Vous êtes un\n\t\t\t\t<select name=\"Statut\" id=\"dropdown\" class=\"field\" v-model=\"statut\">\n\t\t\t\t\t<option value=\"Particulier\">Particulier</option>\n\t\t\t\t\t<option value=\"Professionnel\">Professionnel</option>\n\t\t\t\t</select>\n\t\t\t</label>\n\n\t\t\t<label v-show='statut==\"Professionnel\"' for=\"societe\">Société\n\t\t\t\t<input type=\"text\" name=\"Societe\" id=\"societe\" class=\"field\" v-model=\"soc\">\n\t\t\t</label>\n\n\t\t\t<label for=\"nom\" id=\"nom\">Nom\n\t\t\t\t<input type=\"text\" name=\"Nom\" class=\"field ppl_id\" v-model=\"nom\">\n\t\t\t</label>\n\n\t\t\t<label for=\"prenom\" id=\"prenom\">Prénom\n\t\t\t\t<input type=\"text\" name=\"Prenom\" class=\"field ppl_id\" v-model=\"pren\">\n\t\t\t</label>\n\n\t\t\t<label for=\"tel\" id=\"tel\">Téléphone\n\t\t\t\t<input type=\"tel\" name=\"Tel\" class=\"field\" placeholder=\"01 23 45 67 89\" v-model=\"tel\">\n\t\t\t</label>\n\n\t\t\t<label for=\"email\" id=\"email\">Email\n\t\t\t\t<input type=\"email\" name=\"Email\" class=\"field\" placeholder=\"email@exemple.com\" v-model=\"email\">\n\t\t\t</label>\n\n\t\t\t<label for=\"objet\" id=\"objet\">Objet\n\t\t\t\t<input type=\"text\" name=\"Objet\" class=\"field\" placeholder=\"[Type de contrat] Détail de la demande\" v-model=\"objet\">\n\t\t\t</label>\n\n\t\t\t<textarea name=\"Contenu\" id=\"contenu\" class=\"field\" v-model=\"content\"/>\n\n\t\t\t<input type=\"submit\" id=\"submit\" value=\"Envoi\" class=\"field\">\n\t\t</form>\n\t\t<FooterBar/>\n\t</div>\n</template>\n\n<style scoped>\n\t.content {\n\t\tmin-height: 71vh;\n\t}\n\t#contact {\n\t\tbackground: rgba(255, 255, 255, 0.20);\n\t\tgap: 2vh;\n\t\tmargin: 2vh auto;\n\t\tborder-radius: 16px;\n\t\tbox-shadow: 0 4px 30px rgba(0, 0, 0, 0.1);\n\t\tbackdrop-filter: blur(9px);\n\t\t-webkit-backdrop-filter: blur(9px);\n\t\tborder: 1px solid rgba(255, 255, 255, 1);\n\n\t\tpadding: 2vh;\n\t\tdisplay: grid;\n\t\tgrid-template-columns: repeat(2, 1fr);\n\t\tgrid-template-rows: repeat(7, 1fr);\n\t\talign-items: space-between;\n\t}\n\t#dropdown{grid-row: 1;}\n\t#nom, #prenom{grid-row: 2;}\n\t#tel, #email{grid-row: 3;}\n\t#objet {grid-column: 1/3;}\n\t#contenu {grid-column: 1/3;grid-row: 5/8; height: auto;}\n\t#submit {grid-column: 2; grid-row: 8;width: 100%}\n\n\t@media screen and (max-width: 1024px){\n\t\t#contact {\n\t\t\tdisplay: flex;\n\t\t\tflex-direction: column;\n\t\t}\n\t\t#contenu {\n\t\t\tpadding: 1vw;\n\t\t\tbox-sizing: border-box;\n\t\t\theight: 30vh;\n\t\t}\n\t\t#submit {width: 50%;}\n\t}\n\n\t#contenu {\n\t\tresize: none;\n\t}\n\tlabel {display:flex; flex-direction: column;}\n\tinput[type=\"submit\"] {\n\t\tbackground-color: #fff;\n\t\talign-self: end;\n\t\theight: 5vh;\n\t\twidth: 5vw;\n\t\ttransform-origin: 0 0;\n\t\ttransition: transform 0.5s ease, box-shadow 0.5s ease;\n\t}\n\tinput[type=\"submit\"]:hover {\n\t\ttransform : translateY(-0.5rem);\n\t\tbox-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.375);\n\t}\n\t.field {\n\t\theight: 100%;\n\t\tborder: 1px solid rgba(128, 128, 128, 0.5);\n\t\tborder-radius: 0.2vw;\n\t\ttext-indent: 0.5vw;\n\t}\n\t.field:focus{\n\t\tborder: 1px solid rgb(128, 128, 128);\n\t\toutline: none;\n\t}\n</style>"],"mappings":"AACA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,eAAe;EACXC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IAAEF;EAAU,CAAC;EACzBG,IAAIA,CAAA,EAAG;IACH,OAAQ;MACJC,OAAO,EAAE;IACb,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACNC,KAAK,EAAE;MACHC,GAAGA,CAAA,EAAG;QAAE,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,GAAG;MAAE,CAAC;MACvCC,GAAGA,CAACC,KAAK,EAAE;QAAE,IAAI,CAACJ,MAAM,CAACK,MAAM,CAAC,SAAS,EAAED,KAAK,CAAC;MAAE;IACvD,CAAC;IACDE,MAAM,EAAE;MACJP,GAAGA,CAAA,EAAG;QAAE,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACM,GAAG;MAAE,CAAC;MACvCJ,GAAGA,CAACC,KAAK,EAAE;QAAE,IAAI,CAACJ,MAAM,CAACK,MAAM,CAAC,YAAY,EAAED,KAAK,CAAC;MAAE;IAC1D,CAAC;IACDI,GAAG,EAAE;MACDT,GAAGA,CAAA,EAAG;QAAE,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACO,GAAG;MAAE,CAAC;MACvCL,GAAGA,CAACC,KAAK,EAAE;QAAE,IAAI,CAACJ,MAAM,CAACK,MAAM,CAAC,SAAS,EAAED,KAAK,CAAC;MAAE;IACvD,CAAC;IACDK,GAAG,EAAE;MACDV,GAAGA,CAAA,EAAG;QAAE,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACQ,GAAG;MAAE,CAAC;MACvCN,GAAGA,CAACC,KAAK,EAAE;QAAE,IAAI,CAACJ,MAAM,CAACK,MAAM,CAAC,SAAS,EAAED,KAAK,CAAC;MAAE;IACvD,CAAC;IACDM,IAAI,EAAE;MACFX,GAAGA,CAAA,EAAG;QAAE,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACS,IAAI;MAAE,CAAC;MACxCP,GAAGA,CAACC,KAAK,EAAE;QAAE,IAAI,CAACJ,MAAM,CAACK,MAAM,CAAC,SAAS,EAAED,KAAK,CAAC;MAAE;IACvD,CAAC;IACDO,GAAG,EAAE;MACDZ,GAAGA,CAAA,EAAG;QAAE,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACU,GAAG;MAAE,CAAC;MACvCR,GAAGA,CAACC,KAAK,EAAE;QAAE,IAAI,CAACJ,MAAM,CAACK,MAAM,CAAC,SAAS,EAAED,KAAK,CAAC;MAAE;IACvD,CAAC;