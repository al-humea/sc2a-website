{"ast":null,"code":"export default {\n  name: \"Carousel\",\n  props: {\n    statut: {\n      type: String,\n      required: true\n    },\n    items: {\n      type: Array,\n      required: true\n    }\n  },\n\n  data() {\n    return {\n      current: 0,\n      showText: false\n    };\n  },\n\n  methods: {\n    toSlide(num) {\n      this.current = num - 1;\n    },\n\n    incrementSlider() {\n      this.current++;\n\n      if (this.current == this.items.length) {\n        this.current = 0;\n      }\n    },\n\n    decrementSlider() {\n      this.current--;\n      if (this.current < 0) this.current = this.items.length - 1;\n    },\n\n    contacter(name) {\n      this.$store.commit(\"UPD_TRAN\", \"slideRight\");\n      this.$store.commit(\"UPD_CURPAGE\", 4);\n      this.$store.dispatch(\"updateRequest\", {\n        obj: \"[\" + name + \"] Détail de la demande\",\n        status: this.statut\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AACC,eAAe;EACdA,IAAG,EAAI,UADO;EAEdC,KAAK,EAAE;IACNC,MAAM,EAAE;MACPC,IAAI,EAAEC,MADC;MAEPC,QAAQ,EAAE;IAFH,CADF;IAKNC,KAAI,EAAI;MACPH,IAAG,EAAII,KADA;MAEPF,QAAQ,EAAE;IAFH;EALF,CAFO;;EAYdG,IAAI,GAAG;IACN,OAAQ;MACPC,OAAO,EAAE,CADF;MAEPC,QAAQ,EAAE;IAFH,CAAR;EAIA,CAjBa;;EAkBdC,OAAO,EAAE;IACRC,OAAO,CAACC,GAAD,EAAK;MACX,KAAKJ,OAAL,GAAeI,GAAE,GAAI,CAArB;IACA,CAHO;;IAIRC,eAAe,GAAE;MAChB,KAAKL,OAAL;;MACA,IAAI,KAAKA,OAAL,IAAgB,KAAKH,KAAL,CAAWS,MAA/B,EAAsC;QACrC,KAAKN,OAAL,GAAe,CAAf;MACD;IACA,CATO;;IAURO,eAAe,GAAE;MAChB,KAAKP,OAAL;MACA,IAAI,KAAKA,OAAL,GAAe,CAAnB,EACC,KAAKA,OAAL,GAAe,KAAKH,KAAL,CAAWS,MAAX,GAAoB,CAAnC;IACD,CAdO;;IAeRE,SAAS,CAACjB,IAAD,EAAM;MACd,KAAKkB,MAAL,CAAYC,MAAZ,CAAmB,UAAnB,EAA+B,YAA/B;MACA,KAAKD,MAAL,CAAYC,MAAZ,CAAmB,aAAnB,EAAkC,CAAlC;MACA,KAAKD,MAAL,CAAYE,QAAZ,CAAqB,eAArB,EAAqC;QACpCC,GAAG,EAAC,MAAMrB,IAAN,GAAY,wBADoB;QAEpCsB,MAAM,EAAE,KAAKpB;MAFuB,CAArC;IAID;;EAtBQ;AAlBK,CAAf","names":["name","props","statut","type","String","required","items","Array","data","current","showText","methods","toSlide","num","incremen